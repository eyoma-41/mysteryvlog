{
  "rewrites": [
    { "source": "/", "destination": "/intro.html" }
  ],
  "redirects": [
    { "source": "/index.html", "destination": "/main.html", "permanent": false }
  ],
  "headers": [
    {
      "source": "/behind.html",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, max-age=0" }
      ]
    }
  ]
}
